PROTOCOL AMP_Guaranteed
PROCESSES: 3
STATE:
    x ∈ {0,1}
INITIAL VALUES:
    [0.0, 0.5, 1.0]
PARAMETERS:
    y ∈ [0,1] = 0.5
CHANNEL:
    stochastic

MODEL:
    guaranteed k=2 scope=per_round

UPDATE RULE:
    EACH ROUND:
        if received_diff then
            xᵢ ← y
        else
            xᵢ ← x
        end

METRICS:
    discrepancy
    consensus
